{
  "dependencies": {
    "@sentry/browser": "5.9.1",
    "animate.css": "3.7.2",
    "blueimp-load-image": "2.24.0",
    "classnames": "2.2.6",
    "connected-react-router": "6.6.0",
    "history": "4.10.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-ga": "2.7.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "redux": "4.0.4",
    "redux-actions": "2.6.5"
  },
  "devDependencies": {
    "@svgr/webpack": "4.3.3",
    "@types/autoprefixer": "9.6.1",
    "@types/blueimp-load-image": "^2.23.4",
    "@types/classnames": "2.2.9",
    "@types/copy-webpack-plugin": "5.0.0",
    "@types/history": "4.7.3",
    "@types/html-webpack-plugin": "3.2.1",
    "@types/jest": "24.0.23",
    "@types/mini-css-extract-plugin": "0.8.0",
    "@types/node": "12.12.14",
    "@types/optimize-css-assets-webpack-plugin": "5.0.1",
    "@types/react": "16.9.15",
    "@types/react-dom": "16.9.4",
    "@types/react-redux": "7.1.5",
    "@types/react-router-dom": "5.1.2",
    "@types/redux-actions": "2.6.1",
    "@types/terser-webpack-plugin": "2.2.0",
    "@types/webpack-merge": "4.1.5",
    "@typescript-eslint/eslint-plugin": "2.9.0",
    "@typescript-eslint/parser": "2.9.0",
    "autoprefixer": "9.7.3",
    "copy-webpack-plugin": "5.0.5",
    "css-loader": "3.2.0",
    "css-modules-typescript-loader": "4.0.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "file-loader": "5.0.2",
    "html-webpack-plugin": "4.0.0-beta.11",
    "husky": "3.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "lint-staged": "9.4.3",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.13.0",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "run-script-os": "1.0.7",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "terser-webpack-plugin": "2.2.1",
    "ts-jest": "24.2.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.5.2",
    "typescript": "3.7.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0",
    "webpack-merge": "4.2.2"
  },
  "engine": {
    "node": "12.13.1",
    "npm": "6.13.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-hooks"
    ],
    "rules": {
      "@typescript-eslint/interface-name-prefix": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "\\.svg$": "@svgr/webpack",
      "\\.scss$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ]
  },
  "lint-staged": {
    "**/*.(html|json|md|yml|scss)": [
      "npm run prettify:static",
      "git add"
    ],
    "**/*.(ts|tsx)": [
      "npm run prettify:typescript",
      "git add"
    ]
  },
  "name": "react-boilerplate",
  "private": true,
  "scripts": {
    "build": "npm run clean && run-script-os",
    "build:default": "NODE_ENV=production webpack",
    "build:win32": "set NODE_ENV=production webpack",
    "clean": "rimraf build",
    "prettify": "npm-run-all prettify:*",
    "prettify:static": "prettier --ignore-path .gitignore --write \"**/*.{html,json,md,scss}\"",
    "prettify:typescript": "eslint --ignore-path .gitignore --fix . --ext .ts --ext .tsx",
    "start": "webpack-dev-server --host 0.0.0.0 --port 3000",
    "test": "jest"
  },
  "version": "0.1.0"
}
